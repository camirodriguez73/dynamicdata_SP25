<div class="row owl-carousel owl-theme">
    {{#each destinations}}
    <div class="card">
        <img src="{{this.image}}" class="card-img-top" alt="{{this.name}}">
        <div class="card-body">
            <h3> {{this.name}}</h3>
            <a href="{{this.googleMap}}" class="btn btn-primary">Google Maps</a>
            <p class="card-text">{{this.description}}</p>
        </div>
    </div>
    {{/each}}
</div>